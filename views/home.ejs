<%- include("partials/header") -%>
<h1>
    Home
</h1>
<p> <%= homeStartingContent  %> </p>

<% posts.forEach(function(post){ %>
<div class="item">
    <h3> <%= post.title %> </h3>
    <p>
        <%= post.content.substring(0, 100) + "..." %> <a href="/posts/<%= post.title %> ">Read More</a>
    </p>
</div>
<% }); %>


<%- include("partials/footer") -%>